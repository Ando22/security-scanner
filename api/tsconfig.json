{
  "ts-node": {
    "require": [
      "tsconfig-paths/register"
    ],
    "compilerOptions": {
      "module": "CommonJS"
    }
  },
  "compilerOptions": {
    "module": "ESNext",
    "lib": [
      "ES5",
      "ES6"
    ],
    "strict": true,
    "moduleResolution": "node",
    "target": "es6",
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
      "*": [
        "./*"
      ]
    },
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true
  },
  "include": [
    "./**/*.ts"
, "main.ts"  ],
  "exclude": [
    "node_modules"
  ]
}